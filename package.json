  },
  "homepage": "https://github.com/scrollback/pure#readme",
  "ava": {
    "files": [
      "**/*.test.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill",
      "./test/helpers/setup-browser-env"
    ],
    "babel": "inherit"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
